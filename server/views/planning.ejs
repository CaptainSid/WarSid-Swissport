    <script>
        $(document.body).hide();
        var show = false;
        var token = localStorage.getItem('token');
        if (token != null || token != undefined) {
            $.ajaxSetup({
                beforeSend: function (xhr) {
                    xhr.setRequestHeader('authorization', 'bearer ' + token);
                }
            });
        }

        $.ajax({
            type: "GET",
            url: "/api/token",
            success: function () {
                show = true;
            },
            error: function () {
                window.location.replace("/api/login");
            },
            async: false
        })


        if (show) {
            $(document.body).show();
        }
    </script>
